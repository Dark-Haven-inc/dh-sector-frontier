<DefaultWindow xmlns="https://spacestation14.io" MinSize="360 160" Title="{Loc 'sector-capture-title'}">
  <BoxContainer Orientation="Vertical" SeparationOverride="8" Margin="8 8 8 8" HorizontalExpand="True" VerticalExpand="True">
    <ProgressBar Name="Progress" MinValue="0" MaxValue="1" Page="0.01" Value="0" HorizontalExpand="True" />
    <Button Name="StartButton" Text="{Loc 'sector-capture-start'}" HorizontalExpand="True" />
    <Label Name="StatusLabel" Text="" HorizontalAlignment="Left" />
  </BoxContainer>
</DefaultWindow>


